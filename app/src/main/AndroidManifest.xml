<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cookapptheme">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ChangePasswordActivity"></activity>
        <activity android:name=".AccountSettingActivity" />
        <activity android:name=".UpdateInfoActivity" />
        <activity
            android:name=".MyPageActivity"
            android:label="@string/title_activity_my_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".FragmentActivity" />
        <activity android:name=".SignInAccountActivity" />
        <activity android:name=".SignUpAccountActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>